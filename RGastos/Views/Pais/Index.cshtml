@model IEnumerable<RGastos.Models.Pais>

@{
    ViewBag.Title = "Listado Países";
}

<h2>Listado.Países</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="bordered">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Nombre)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Nombre)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.PaisID }) |
            @Html.ActionLink("Details", "Details", new { id=item.PaisID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.PaisID })
        </td>
    </tr>
}

</table>
